<record id="fruit_juice_production_report" model="ir.actions.report">
    <field name="name">Fruit Juice Production</field>
    <field name="model">fruit.juice.production</field>
    <field name="report_name">fruit.juice.production</field>
    <field name="report_type">py3o</field>
    <field name="py3o_filetype">odt</field>
    <field name="module">fruit_mill</field>
    <field name="py3o_template_fallback">report/fruit_juice_production.odt</field>
    <field name="print_report_name">'Fruit_juice_production_' + object.name + '.odt'</field>
    <field name="py3o_multi_in_one" eval="True"/>
</record>

<record id="button_fruit_juice_production_report" model="ir.actions.server">
    <field name="key2">client_print_multi</field>
    <field name="model_id" ref="fruit_mill.fruit_juice_production_report"/>
    <field name="name">Fruit Juice Production</field>
    <field name="model_binding" ref="base.model_ir_actions_report"></field>
    <field name="value" eval="'ir.actions.report,%d'%fruit_juice_production_report" />
</record>

